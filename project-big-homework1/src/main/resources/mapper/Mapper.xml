<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--//-->
<mapper namespace="com.backed.mapper.StudentMapper">

  <select id="query__" resultType="com.backed.pojo.Client">
    select * from student
  </select>

  <select id="login__" resultType="com.backed.pojo.Client">
    select * from student where id = #{id} and name = #{name}
  </select>

  <insert id="add">
    insert into student (id,name)values (#{id},#{name})
  </insert>


<!--  http://localhost/demo_war_lb/student/update?id=2020080211&CAG=2005&name=father-->
</mapper>
